[submodule ".tmux/tmux-colors-solarized"]
  path = .tmux/tmux-colors-solarized
  url = https://github.com/seebi/tmux-colors-solarized.git
[submodule ".tmux/tmux-powerline.git"]
  path = .tmux/tmux-powerline.git
  url = git://github.com/erikw/tmux-powerline.git
[submodule ".powerline/fonts"]
	path = .powerline/fonts
	url = https://github.com/powerline/fonts.git
[submodule ".tmux/plugins/tpm"]
	path = .tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule ".proj-commander"]
	path = .proj-commander
	url = git@github.com:nickdiego/proj-commander.git
	ignore = dirty
[submodule ".xdg-config/awesome-config"]
	path = .xdg-config/awesome-config
	url = git@github.com:nickdiego/awesome-confg.git
[submodule ".xdg-config/base16-shell"]
	path = .xdg-config/base16-shell
	url = https://github.com/chriskempson/base16-shell.git
